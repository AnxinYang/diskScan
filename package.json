{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "tsc": "tsc",
    "build": "webpack -p --config webpack.config.production.js",
    "server": "webpack-dev-server --config webpack.config.dev.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "scss": "sass --watch src/main.scss build/main.css"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^12.7.4",
    "electron": "^6.0.7"
  },
  "dependencies": {
    "ccts": "^1.1.9"
  }
}